{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "styles/*.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/submit_form", "dest": "/api/submit_form.js" },
    { "src": "/contact", "dest": "/contact.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
